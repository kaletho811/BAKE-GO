<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/fondo">

    <ImageView
        android:id="@+id/ic_flecha_edit_usu"
        android:layout_width="69dp"
        android:layout_height="63dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="28dp"
        android:clickable="true"
        android:contentDescription="@string/ir_atras"
        android:focusable="true"
        app:layout_constraintEnd_toStartOf="@+id/nom_App_edit_usu"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_flecha_atras" />

    <TextView
        android:id="@+id/nom_App_edit_usu"
        android:layout_width="226dp"
        android:layout_height="69dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="28dp"
        android:fontFamily="@font/kavoon"
        android:text="@string/bake_go"
        android:textAlignment="viewStart"
        android:textColor="@color/black"
        android:textSize="48sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.649"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/titulo_edit_usu"
        android:layout_width="251dp"
        android:layout_height="37dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        android:fontFamily="@font/kavoon"
        android:text="@string/editar_usuarios"
        android:textAlignment="viewStart"
        android:textColor="@color/black"
        android:textSize="28sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.473"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/nom_App_edit_usu" />

</androidx.constraintlayout.widget.ConstraintLayout>